<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.reda.mysqltest">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Main2Activity" >
        <intent-filter>
            <action android:name="com.example.reda.mysqltest.Main2Activity" />

            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>
        </activity>

        <activity android:name=".Chat" >
        <intent-filter>
            <action android:name="com.example.reda.mysqltest.Chat" />

            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>
        </activity>


        <activity android:name=".Picture">
            <intent-filter>
                <action android:name="android.intent.action.Picture" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>

        </activity>

    </application>

</manifest>